package Buoi2;

import java.util.Scanner;

public class Ls1 {

	public static void main(String[] args) {
		printStarts(5);
		
		

	}
	public static void printStarts(int n) {
	int i,j;
	for(i=0;i<n;i++)
	{
		for(j=0;j<i;j++)
		{
			if(i==n-1||j==0) 
			System.out.print(" * ");
			else
				System.out.print("");
			
		}
		System.out.println();
	}
	}

}
